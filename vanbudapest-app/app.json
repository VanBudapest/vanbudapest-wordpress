{
  "expo": {
    "name": "VanBudapest",
    "slug": "vanbudapest-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2E7D32"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vanbudapest.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2E7D32"
      },
      "package": "com.vanbudapest.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "wordpressApiUrl": "https://public-api.wordpress.com/rest/v1.1/sites/vanbudapest.com"
    }
  }
}
